with (obj_cursor)
    {
    x = mouse_x;
    y = mouse_y;
    }

if (keyboard_check_pressed(vk_tab))
    {
    if (song) FMODInstanceSetPaused(songinstance,1);
    room_goto(rm_ilda);
    }
    
